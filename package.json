{
  "name": "metapatcher",
  "version": "v3.0.0",
  "description": "Device aware HTML document head management including meta tags, social media tags, icons and JSONLD expressions.",
  "main": "./dist/browser/cjs/index.js",
  "module": "./dist/browser/es/index.js",
  "type": "module",
  "types": "types",
  "exports": {
    ".": {
      "import": "./dist/browser/es/index.js",
      "require": "./dist/browser/cjs/index.js"
    },
    "./browser": {
      "import": "./dist/browser/es/index.js",
      "require": "./dist/browser/cjs/index.js"
    }
  },
  "scripts": {
    "precompile": "rm -rf build/*",
    "compile": "tsc -p tsconfig.json",
    "build": "rollup --config rollup.config.js",
    "test": "NODE_ENV=test jasmine --config=spec/config/browser.json && jasmine --config=spec/config/node.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muratgozel/metapatcher.git"
  },
  "keywords": [
    "html-document",
    "html-head-generator",
    "meta-tags",
    "social-media-icons",
    "opengraph-tags",
    "jsonld"
  ],
  "author": {
    "name": "Murat GÃ¶zel",
    "email": "murat@gozel.com.tr",
    "url": "https://gozel.com.tr"
  },
  "funding": {
    "type": "ko-fi",
    "url": "https://ko-fi.com/muratgozel"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muratgozel/metapatcher/issues"
  },
  "homepage": "https://github.com/muratgozel/metapatcher#readme",
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.6",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.1",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "core-js": "^3.18.3",
    "dom-scripter": "^5.0.0",
    "eslint": "^8.38.0",
    "jasmine": "^4.6.0",
    "jsdom": "^21.1.1",
    "node-releaser": "^2.0.0-beta.4",
    "rollup": "^3.20.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.4",
    "@babel/runtime-corejs3": "^7.15.4",
    "@tsconfig/esm": "^1.0.2",
    "@tsconfig/node18": "^1.0.1",
    "@tsconfig/strictest": "^2.0.0"
  }
}